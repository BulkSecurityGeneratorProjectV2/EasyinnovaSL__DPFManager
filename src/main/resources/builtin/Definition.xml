<?xml version="1.0" encoding="UTF-8"?>
<tns:ListOutput xmlns:tns="http://www.preforma-project/interoperability"
                xmlns:schemaLocation="http://www.preforma-project/interoperability preformainteroperability.xsd"
                xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <ConformanceCheckers>
        <conformanceChecker>
            <name>Tiff</name>
            <location>built-in</location>
            <parameters></parameters>
            <state>enabled</state>
            <defaultConfiguration></defaultConfiguration>
            <extensions>
                <extension>tif</extension>
                <extension>tiff</extension>
            </extensions>
        </conformanceChecker>
        <conformanceChecker>
            <name>VeraPDF</name>
            <location>C:\Program Files\DPF Manager\plugins\pdf\verapdf.bat</location>
            <parameters>--format xml %input% %config%</parameters>
            <state>disabled</state>
            <defaultConfiguration></defaultConfiguration>
            <extensions>
                <extension>pdf</extension>
            </extensions>
        </conformanceChecker>
        <conformanceChecker>
            <name>MediaConch</name>
            <location>C:\Program Files\DPF Manager\plugins\video\MediaConch.exe</location>
            <parameters>-mc -fx %input% %config%</parameters>
            <state>enabled</state>
            <defaultConfiguration></defaultConfiguration>
            <extensions>
                <extension>mov</extension>
            </extensions>
        </conformanceChecker>
    </ConformanceCheckers>
</tns:ListOutput>