<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<AnchorPane fx:id="task" xmlns="http://javafx.com/javafx/null" xmlns:fx="http://javafx.com/fxml/1">
    <VBox fx:id="vbox" styleClass="task-box" alignment="TOP_CENTER" onMouseClicked="#showReport" AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="5.0">
        <children>
            <Label fx:id="input" styleClass="label-info" text="Label" />
            <ProgressBar styleClass="progress-bar" onMouseClicked="#showReport" fx:id="progress" progress="0.0" />
        </children>
        <VBox.margin>
            <Insets left="10.0" right="10.0" />
        </VBox.margin>
    </VBox>
</AnchorPane>
