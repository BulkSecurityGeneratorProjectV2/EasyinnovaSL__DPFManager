<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.web.*?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.web.WebView?>

<AnchorPane id="pane-show" prefWidth="970.0" styleClass="background-main" xmlns="http://javafx.com/javafx/null" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <VBox fx:id="showVBox" alignment="TOP_CENTER" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <TextArea fx:id="textArea" VBox.vgrow="ALWAYS">
            <styleClass>
               <String fx:value="textArea" />
               <String fx:value="dpf-bar" />
            </styleClass></TextArea>
            <WebView fx:id="webView" styleClass="webView" VBox.vgrow="ALWAYS" />
        </VBox>
      <ComboBox fx:id="comboIndividuals" onAction="#changeIndividual" prefWidth="150.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="10.0" />
    </children>
</AnchorPane>
